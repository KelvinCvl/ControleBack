datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Stats {
  id                  Int      @id @default(autoincrement())
  totalSpecies        Int
  totalObservations   Int
  averagePerSpecies   Float
  createdAt           DateTime @default(now())
}

